# Environment Variables for BallotDAO

# Network RPC URLs
VITE_SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY

# Contract Addresses (will be filled after deployment)
VITE_SEPOLIA_BALLOT_DAO=
VITE_SEPOLIA_BALLOT_TOKEN=
VITE_SEPOLIA_DAO_TREASURY=

# WalletConnect Project ID (from https://cloud.walletconnect.com/)
VITE_WALLETCONNECT_PROJECT_ID=

# Alchemy API Key (for Sepolia testnet)
VITE_ALCHEMY_API_KEY=

# Etherscan API Key (for contract verification)
VITE_ETHERSCAN_API_KEY=
